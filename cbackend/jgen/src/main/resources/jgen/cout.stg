unit_body(unit) ::= <<#include "<unit.name>.h"
<classes_impl(unit.classes)>

>>

classes_impl(classes) ::= "<classes:impl_class();separator=\"\n\n\">"

impl_class(cls) ::= <<typedef struct tag_<cls.name> {

} <cls.name>;
>>


unit_header(unit) ::= <<pragma once
#include "src/include/miod_builtins/miod_builtins.h"
<unit.imports:include_import();separator="\n">
>>

include_import(unit_name) ::= "#include \"<unit_name>.h\""
